<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_graph"
  app:startDestination="@id/mainFragment">
  <fragment
    android:id="@+id/mainFragment"
    android:name="com.jasonstarling.finalproject.MainFragment"
    android:label="fragment_main"
    tools:layout="@layout/fragment_main">
    <action
      android:id="@+id/action_mainFragment_to_foodFragment"
      app:destination="@id/foodFragment" />
    <action
      android:id="@+id/action_mainFragment_to_symptomFragment"
      app:destination="@id/symptomFragment" />
  </fragment>
  <fragment
    android:id="@+id/foodFragment"
    android:name="com.jasonstarling.finalproject.FoodEntryAdd"
    android:label="fragment_food_entry_add"
    tools:layout="@layout/fragment_food_entry_add">
    <action
      android:id="@+id/action_foodFragment_to_mainFragment"
      app:destination="@id/mainFragment" />
  </fragment>
  <fragment
    android:id="@+id/symptomFragment"
    android:name="com.jasonstarling.finalproject.SymptomEntryAdd"
    android:label="fragment_symptom_entry_add"
    tools:layout="@layout/fragment_symptom_entry_add">
    <action
      android:id="@+id/action_symptomFragment_to_mainFragment"
      app:destination="@id/mainFragment" />
  </fragment>
</navigation>